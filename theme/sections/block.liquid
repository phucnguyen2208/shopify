//cart quantity
$(document).ready(function(){
  var cartCount = {{ cart.item_count }};
  $(".add-to-cart").on("click",function(){
    jQuery.getJSON("/cart.js",function(){
      cartCount++;
      $(".cart-count").html(cartCount)
    })
  })
});





